{
    "files.exclude": {
        "**/pubspec.lock": true,
        ".flutter-plugins": true,
        ".flutter-plugins-dependencies": true,
        ".metadata": true,
        ".idea": true,
        "windows": false,
        "ios": true,
        "web": false,
        "linux": false,
        "macos": false,
        "build": false,
        "**/.dart_tool": true,
        "intern_kassation_app.iml": true,
        ".run": true,
        ".sentry-native": true,
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/env": true,
        "**/venv": true,
        "**/build": true,
        "**/.dart_tool": true,
    },
    "files.watcherExclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/env": true,
        "**/venv": true,
        "**/build": true,
        "**/.dart_tool": true,
    },
    "editor.codeActionsOnSave": {
        //"source.fixAll": "explicit",
        "editor.formatOnSave": "explicit"
    },
    "terminal.integrated.suggest.enabled": false,
    "editor.bracketPairColorization.enabled": true,
    "dart.previewFlutterUiGuides": true,
    "editor.stickyScroll.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.dart": "${capture}.g.dart, ${capture}.freezed.dart, ${capture}.steps.dart, ${capture}.config.dart, ${capture}.mapper.dart"
    },
    "arb-editor.suppressedWarnings": [
        "missing_metadata_for_key",
        "metadata_for_missing_key"
    ],
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#ab307e",
        "activityBar.background": "#ab307e",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#25320e",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#ab307e",
        "statusBar.background": "#832561",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#ab307e",
        "statusBarItem.remoteBackground": "#832561",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#832561",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#83256199",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "material-icon-theme.folders.associations": {
        "bloc": "bloc",
        "cubit": "bloc",
        "setup": "context",
        "data_providers": "api",
        "domain": "base",
        "local": "client",
        "entities": "interface",
        "bootstrap": "context",
        "dependency_injection": "context",
        "features": "batch"
    },
    "files.associations": {
        "xstring": "cpp"
    },
    "peacock.color": "#832561"
}